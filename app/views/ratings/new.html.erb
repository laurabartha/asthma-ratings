<h1>New Rating</h1>
<%= form_for :rating, url: articles_path do |f| %>
  <%= f.label :address %>:
  <%= f.text_field :streetnumber %><br />
  <%= f.text_field :street %><br />
  <%= f.text_field :unit %><br />
  <%= f.text_field :city %><br />
  <%= f.text_field :country %><br />

  <%= f.label :rating %>:
  <%= f.text_field :rating %><br />

  <%= f.label :comment  %>:
  <%= f.text_field :comment %><br />

  <%= f.submit %>
<% end %>
